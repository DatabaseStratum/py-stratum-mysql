/**
 * Test for designation type row0.
 *
 * @param p_count The number of rows selected.
 *                * 0 For a valid test.
 *                * 1 For a valid test.
 *                * 2 For an invalid test.
 *
 * @type row0
 */
create procedure tst_test_row0a( p_count int )
reads sql data
begin
  select *
  from   TST_FOO2
  where  tst_c00 <= p_count
  ;
end
