/**
 * Test for designation type singleton0.
 *
 * @param p_count The number of rows selected.
 *                * 0 For a valid test.
 *                * 1 For a valid test.
 *                * 2 For an invalid test.
 *
 * @type singleton0
 */
create procedure tst_test_singleton0a( p_count int )
reads sql data
begin
  select 1
  from   TST_FOO2
  where  tst_c00 <= p_count
  ;
end
